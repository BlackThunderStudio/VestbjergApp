<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.102"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.teamSuperior.guiApp.controller.ProductsController">
    <children>
        <TableView fx:id="tableView_products" onMouseClicked="#tableView_products_onClick" prefHeight="400.0"
                   prefWidth="1000.0">
        </TableView>
        <PieChart fx:id="chart_storageCap" layoutY="400.0" prefHeight="400.0" prefWidth="400.0"
                  title="Storage Capacity"/>
    </children>
</AnchorPane>
